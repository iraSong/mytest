<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="renderer" content="webkit">
  <meta http-equiv="Cache-Control" content="no-cache">
  <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="format-detection" content="telephone=no">
  <meta name="screen-orientation" content="portrait">
  <meta name="x5-orientation" content="portrait">
  <link rel="bookmark" type="image/x-icon" href="{{config 'client.publicPath'}}/static/favicon.ico?_={{config 'version'}}">
  <link rel="icon" type="image/x-icon" href="{{config 'client.publicPath'}}/static/favicon.ico?_={{config 'version'}}">
  <link rel="shortcut icon" type="image/x-icon" href="{{config 'client.publicPath'}}/static/favicon.ico?_={{config 'version'}}">
  <link rel="icon" type="image/png" sizes="64x64" href="{{config 'client.publicPath'}}/static/favicon.png?_={{config 'version'}}">
  <link rel="apple-touch-icon" sizes="64x64" href="{{config 'client.publicPath'}}/static/favicon.png?_={{config 'version'}}">
  <title>{{title}}</title>
  <script src="{{config 'client.publicPath'}}/static/detect-browser.js?_={{config 'version'}}"></script>
  <script src="{{config 'client.publicPath'}}/static/screen-adapter.js?_={{config 'version'}}"></script>
  {{#if style}}
    <link rel="stylesheet" href="{{config 'client.publicPath'}}/static{{style}}?_={{config 'version'}}">
  {{/if}}
  <script>
    (function(){
      var ua = window.navigator.userAgent;
      var browserCls = [];
      if (/android/i.test(ua)) {
        browserCls.push('android')
      }
      if (/os x/i.test(ua)) {
        browserCls.push('ios')
      }
      if (/micromessenger/i.test(ua)) {
        browserCls.push('wechat')
      }
      if (/goldbeta/i.test(ua)) {
        browserCls.push('goldbeta')
      }
      if (window.isMobile) {
        browserCls.push('mobile')
      } else {
        browserCls.push('desktop')
      }
      document.documentElement.className += ' ' + browserCls.join(' ')
    }());
  </script>
</head>
<body>
  <div id="loading-animation" style="display:block"></div>
  <div id="body">